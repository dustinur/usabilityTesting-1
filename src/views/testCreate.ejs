<%- include('./includes/head.ejs') %>
</head>

<%- include('./includes/nav.ejs') %>

<body id="page-top">

	<!-- About Section -->
	<section id="about" class="content-section text-center">
		<div class="container">
			<div class="row">
				<div class="col-lg-8 mx-auto">
					<h2>Link to test site and test parameters</h2>
					<p>
						<% if (typeof noLog != 'undefined' && noLog) { %>
						<%=noLog%>
						<% } %>

					</p>
					<h3>Test name:
						<span style="color: #42DCA3;"><%=testName%></span>
					</h3>
					<ul class="list-group mb-5">
						<%for (var i=0; i < ArrayPull.length; i++) {%>
						<li class="list-group-item">
							<p class="" style="color:#000;">
								<%=ArrayPull[i]%>
							</p>
						</li>
						<%}%>
					</ul>

					<h3>Link to send to users:</h3>
					<p><a href="<%=URLPull%>" target="_blank">
							<%=URLPull%></a></p>
					<h3>Results Page:</h3>
					<p><a href="<%=mainPage%>" target="_blank">
							<%=mainPage%></a></p>
				</div>
			</div>
		</div>
	</section>

	<%- include('./includes/footer.ejs') %>

	<%- include('./includes/scripts.ejs') %>


</body>

</html>